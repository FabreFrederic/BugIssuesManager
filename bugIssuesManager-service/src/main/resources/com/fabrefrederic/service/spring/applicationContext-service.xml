<?xml version="1.0" encoding="ISO-8859-1"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="classpath:com/fabrefrederic/dao/spring/applicationContext-dao.xml" />
	<import resource="classpath:com/fabrefrederic/library/spring/applicationContext-library.xml" />
	
	<bean id="commitService"
		class="com.fabrefrederic.service.CommitServiceImpl">
		<property name="revisionControlSystem" ref="revisionControlSystem" />
	</bean>
	<bean id="issueService"
		class="com.fabrefrederic.service.IssueServiceImpl">
		<property name="issueDao" ref="issueDaoHibernate" />
		<property name="commitDao" ref="commitDaoHibernate" />
	</bean>
</beans>
